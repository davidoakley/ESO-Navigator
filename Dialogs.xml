                                    </OnFocusGained>
                                    <OnFocusLost>
<!--                                        ZO_CollectibleRenameDialog_HideViolations(self)-->
                                    </OnFocusLost>
                                </EditBox>
                            </Controls>
                        </Backdrop>

<!--                        <Control name="$(parent)Dropdown" inherits="ZO_ComboBox">-->
<!--                            <Dimensions x="340"/>-->
<!--                            <Anchor point="LEFT"/>-->
<!--                        </Control>-->
                    </Controls>
                </Control>

            <Control name="$(parent)Buttons" resizeToFitDescendents="true">
                <Anchor point="TOP" relativeTo="$(parent)Container" relativePoint="BOTTOM" offsetY="33" />
                <Controls>

                    <Control name="$(parent)Default" inherits="ZO_DialogButton">
                        <Anchor point="LEFT" />
                    </Control>
                    <Control name="$(parent)Ok" inherits="ZO_DialogButton">
                        <Anchor point="LEFT" relativeTo="$(parent)Default" relativePoint="RIGHT" offsetX="20"/>
                    </Control>
                    <Control name="$(parent)Cancel" inherits="ZO_DialogButton">
                        <Anchor point="LEFT" relativeTo="$(parent)Ok" relativePoint="RIGHT" offsetX="20"/>
