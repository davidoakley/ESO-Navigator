                <Control name="$(parent)Container" resizeToFitDescendents="true">
                    <OnInitialized>
                        --self.dropdown = ZO_ComboBox_ObjectFromContainer(self:GetNamedChild("Dropdown"))
                    </OnInitialized>
                    <Anchor point="TOPLEFT" relativeTo="$(parent)Text" relativePoint="BOTTOMLEFT" offsetY="16" />
                    <Controls>

<!--                        <Label name="$(parent)NameHeader" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" font="ZoFontGame" text="SI_COLLECTIONS_INVENTORY_DIALOG_RENAME_COLLECTIBLE_MAIN">-->
<!--                            <Anchor point="TOPLEFT" offsetX="-12" />-->
<!--                        </Label>-->

                        <Backdrop name="$(parent)Edit" inherits="ZO_SingleLineEditBackdrop_Keyboard">
<!--                            <Anchor point="TOPLEFT" relativeTo="$(parent)NameHeader" relativePoint="BOTTOMLEFT" offsetY="5"/>-->
                            <Anchor point="LEFT"/>
                            <Dimensions x="340" />
                            <Controls>
                                <EditBox name="$(parent)Box" inherits="ZO_DefaultEditForBackdrop">
                                    <OnTextChanged>
<!--                                        ZO_CollectibleRenameDialog_UpdateViolations(self)-->
                                    </OnTextChanged>
                                    <OnEffectivelyShown>
                                        self:TakeFocus()
                                    </OnEffectivelyShown>
                                    <OnFocusGained>
